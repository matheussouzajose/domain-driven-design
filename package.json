  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "npm test -- --watchAll",
    "test:ci": "npm test -- --coverage",
    "test:cov": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:unit": "npm test -- --watchAll -c jest-unit.config.ts",
    "test:request": "npm test -- --watchAll -c jest-request-config.js",
    "test:integration": "npm test -- --watchAll -c jest-integration.config.ts",
    "test:e2e": "npm test -- -c test/jest-e2e.config.ts",
    "test:e2e:watch": "npm test -- --watchAll -c test/jest-e2e.config.ts",
    "test:staged": "npm test -- --findRelatedTests"
  },
  "keywords": [],
  "author": "",
